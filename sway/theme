set $fbg #4F0A9C
set $ffg #ffffff
set $ubg #333333
set $ufg #888888

# class                 border bg fg indicator child_border
client.focused          $fbg $fbg $ffg $fbg $fbg
client.focused_inactive $ubg $ubg $ufg $ubg $ubg
client.unfocused        $ubg $ubg $ufg $ubg $ubg
client.urgent           $ubg $ubg $ufg $ubg $ubg

